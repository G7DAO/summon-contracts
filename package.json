{
  "name": "achievo-contracts",
  "version": "0.6.1",
  "description": "Smart contracts for achievo",
  "scripts": {
    "localChain": "hardhat --config hardhat.config.ts node",
    "clean": "hardhat --config hardhat.config.ts clean",
    "compile": "hardhat --config hardhat.config.ts compile",
    "accounts": "hardhat --config hardhat.config.ts accounts",
    "generate": "hardhat --config hardhat.config.ts generate",
    "compile:zkSync": "hardhat --config zkSync.config.ts compile --network zkSyncTestnet --force",
    "deployLevelsOnChain:ZkSync": "npm run compile:zkSync && hardhat --config zkSync.config.ts deploy-zksync --network zkSyncTestnet --script deployLevelsBound.ts",
    "deployGameSummary:mumbai": "hardhat --config polygon.config.ts run scripts/deployGameSummary1155.ts --network mumbai",
    "deployGameSummary:polygon": "hardhat --config polygon.config.ts run scripts/deployGameSummary1155.ts --network polygon",
    "deployGameSummary:wadsley": "hardhat --config mantle.config.ts run scripts/deployGameSummary1155.ts --network mantleWadsley",
    "deployGameSummary:mantle": "hardhat --config mantle.config.ts run scripts/deployGameSummary1155.ts --network mantle",
    "deployDiamond:mumbai": "hardhat --config polygon.config.ts run scripts/deploys/001_deployDiamond.ts --network polygonMumbai",
    "deployDiamond:polygon": "hardhat --config polygon.config.ts run scripts/deploys/001_deployDiamond.ts --network polygon",
    "deployDiamond:localhost": "hardhat --config hardhat.config.ts run scripts/deploys/001_deployDiamond.ts --network localhost",
    "deployDiamond:wadsley": "hardhat --config mantle.config.ts  run scripts/deploys/001_deployDiamond.ts --network mantleWadsley",
    "deployDiamond:mantle": "hardhat --config mantle.config.ts run scripts/deploys/001_deployDiamond.ts --network mantle",
    "test": "REPORT_GAS=true hardhat --config hardhat.config.ts test --network hardhat",
    "lint": "eslint --config ./.eslintrc.js --ignore-path ./.eslintignore ./deploy/**/*.ts ./tasks/**/*.ts ./test/**/*.ts ./helpers/**/*.ts",
    "solhint": "solhint ./contracts/**/*.sol",
    "format": "yarn prettier -w ./deploy/**/*.ts ./tasks/**/*.ts ./tests/**/*.ts ./helpers/**/*.ts contracts/**/*.sol ",
    "format:check": "yarn prettier -c ./deploy/**/*.ts ./tasks/**/*.ts ./tests/**/*.ts ./helpers/**/*.ts ./contracts/**/*.sol "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/G7DAO/summon-contracts.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/G7DAO/summon-contracts/issues"
  },
  "homepage": "https://github.com/G7DAO/summon-contracts#readme",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^2.0.1",
    "@swc/core": "^1.3.36",
    "@swc/wasm": "^1.3.36",
    "@types/chai": "^4.3.4",
    "@types/debug": "^4.1.7",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "chai": "^4.3.7",
    "debug": "^4.3.4",
    "eslint": "^8.34.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "hardhat": "^2.12.7",
    "prettier": "^2.8.4",
    "prettier-plugin-solidity": "^1.1.2",
    "solhint": "^3.4.0",
    "solhint-plugin-prettier": "^0.0.5",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@matterlabs/hardhat-zksync-deploy": "^0.6.5",
    "@matterlabs/hardhat-zksync-solc": "^0.4.2",
    "@matterlabs/hardhat-zksync-upgradable": "^0.1.3",
    "@matterlabs/hardhat-zksync-verify": "^0.1.8",
    "@nomicfoundation/hardhat-verify": "^1.1.1",
    "@openzeppelin/contracts": "^4.9.3",
    "@openzeppelin/contracts-upgradeable": "^4.9.3",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2"
  }
}
