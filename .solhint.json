{
    "extends": "solhint:recommended",
    "plugins": [],
    "rules": {
        "avoid-throw": "off",
        "avoid-suicide": "error",
        "avoid-sha3": "warn",
        "no-global-import": "off",
        "no-empty-blocks": "off",
        "no-console": "warn",
        "var-name-mixedcase": "off",
        "reason-string": "off",
        "compiler-version": "off",
        "no-inline-assembly": "off"
    }
}
